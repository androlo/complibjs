{
  "name": "@comp/complibjs",
  "version": "0.9.0",
  "description": "Comparison Framework for JavaScript/TypeScript.",
  "author": {
    "name": "Andreas Olofsson",
    "email": "compfram@gmail.com"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "tsc",
    "prepublishOnly": "tsc",
    "clean": "tsc --build --clean",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/coverage-v8": "^4.0.4",
    "esbuild": "^0.25.11",
    "eslint": "^8.56.0",
    "pure-rand": "^7.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^7.18.0",
    "vitest": "^4.0.4"
  },
  "dependencies": {
    "loglayer": "^8.1.0"
  }
}
